interfaces {
    ge-0/0/0 {
        unit 0 {
            family inet {
                address 10.168.1.1/24;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address 10.0.0.1/32;
            }
        }
    }
}

protocols {
    ospf {
        area 0.0.0.0 {
            interface ge-0/0/0.0;
            interface lo0.0;
        }
    }
    bgp {
        group cawk {
            authentication-key xx
        }
        group cawk1 {
            authentication-key xx
        }
        neighbor 10.168.1.2 {
            authentication-key xx
        }
        neighbor 10.168.1.3 {
            authentication-key xx
        }
    }
}

routing-options {
    static {
        route 0.0.0.0/0 next-hop 10.168.1.254;
    }
}

system {
    host-name MonRouteur;
    domain-name mondomaine.local;
    services {
        ssh;
    }
    login {
        user MonUtilisateur {
            uid 2001;
            class super-user;
            authentication {
                plain-text-password;
            }
        }
    }
    auth-order [ radius password ];
    radius-server {
        10.0.0.1 {
            secret "$9$/g3dHZrJNjQw0XvT4vTgv/"; ## SECRET-DATA
            source-address 192.168.1.1;
        }
    }
}

firewall {
    family inet {
        filter trust-to-trust {
            term allow-internal-traffic {
                from {
                    source-address any;
                    destination-address any;
                    application any;
                }
                then {
                    accept;
                }
            }
        }
        filter trust-to-untrust {
            term allow-ssh {
                from {
                    source-address any;
                    destination-address any;
                    application junos-ssh;
                }
                then {
                    accept;
                }
            }
            term allow-icmp {
                from {
                    source-address any;
                    destination-address any;
                    application junos-ping;
                }
                then {
                    accept;
                }
            }
        }
    }
    interfaces {
        ge-0/0/0.0 {
            family inet {
                filter {
                    input strust-to-trust;
                    output trust-to-trusts;
                }
            }
        }
    }
}

snmp {
    community public {
        authorization write;
    }
}

snmp {
    v3 {
        usm {
            local-engine {
                user <user-name> {
                    authentication-md5 {
                        authentication-password "<authentication_password>";
                    }
                    privacy-aes128 {
                        privacy-password "<privacy_password>";
                    }
                }
            }
        }
        vacm {
            security-to-group {
                security-model usm {
                    security-name <user_name> {
                        group-name <group_name>;
                    }
                }
            }
            access {
                group <group_name> {
                    default-context-prefix;
                    security-model usm {
                        security-level privacy;
                        read-view <view_name>;
                        write-view <view_name>;
                        notify-view <view_name>;
                    }
                }
            }
        }
    }
}
