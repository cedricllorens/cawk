@include %SED_INCLUDE_PATH%

BEGIN {
}

BEGINFILE {
	pass = 0 ;
}

/^ssh server enable/ {
	print_err("sshserver_enable_set.huawei-vrp.gawk",$0,FNR,"high","pass");
	pass = 1;
	nextfile;
}

ENDFILE {
	if ( !pass ) print_err("sshserver_enable_set.huawei-vrp.gawk","ssh server is not set",0,"high","error");
}

END {
}
